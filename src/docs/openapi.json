        "type": "object",
        "description": "分页请求对象",
        "properties": {
          "pageNo": {
            "type": "integer",
            "format": "int32",
            "description": "分页页码"
          },
          "pageSize": {
            "type": "integer",
            "format": "int32",
            "description": "分页大小"
          },
          "query": {
            "$ref": "#/components/schemas/MemoPageRequest",
            "description": "查询条件"
          }
        }
      },
      "MemoResponse": {
        "type": "object",
        "description": "备忘详情响应",
        "properties": {
          "title": {
            "type": "string"
          },
          "subTitle": {
            "type": "string",
            "description": "副备忘标题"
          },
          "content": {
            "type": "string",
            "description": "备忘内容"
          },
          "tags": {
            "type": "array",
            "description": "备忘关联标签对象列表",
            "items": {
              "$ref": "#/components/schemas/MemoTagDomain"
            }
          },
          "tagIds": {
            "type": "array",
            "description": "备忘关联标签列表",
            "items": {
              "type": "integer",
              "format": "int64"
            }
          },
          "orderNumber": {
            "type": "integer",
            "format": "int32",
            "description": "书签排序"
          },
          "createTime": {
            "type": "string",
            "format": "date-time",
            "description": "创建时间"
          },
          "updateTime": {
            "type": "string",
            "format": "date-time",
            "description": "更新时间"
          }
        },
        "required": [
          "title"
        ]
      },
      "MemoTagDomain": {
        "type": "object",
        "description": "备忘标签领域对象",
        "properties": {
          "ID": {
            "type": "integer",
            "format": "int64",
            "description": "主键ID"
