  "components": {
    "schemas": {
      "UpdateRequest": {
        "required": [
          "content",
          "id",
          "orderNumber",
          "subTitle",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "title": {
            "type": "string"
          },
          "subTitle": {
            "type": "string"
          },
          "content": {
            "type": "string"
          },
          "orderNumber": {
            "type": "integer",
            "format": "int32"
          },
          "tagIds": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int64"
            }
          }
        }
      },
      "GenericResult": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "description": "错误代码",
            "format": "int32"
          },
          "message": {
            "type": "string",
            "description": "错误信息"
          },
          "data": {
            "type": "string",
            "description": "响应数据"
          }
        },
        "description": "通用响应对象"
      },
      "CreateRequest": {
        "required": [
          "content",
          "subTitle",
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "subTitle": {
            "type": "string"
          },
          "content": {
            "type": "string"
          },
          "orderNumber": {
            "type": "integer",
            "format": "int32"
          },
          "tagIds": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int64"
            }
          }
        }
      },
      "BookmarkDomain": {
        "type": "object",
        "properties": {
          "ID": {
            "type": "integer",
            "description": "主键ID",
            "format": "int64"
          },
          "parentId": {
            "type": "integer",
            "description": "父级ID",
            "format": "int64"
          },
          "createTime": {
            "type": "string",
            "description": "创建时间",
            "format": "date-time"
          },
          "updateTime": {
            "type": "string",
            "description": "更新时间",
            "format": "date-time"
          },
          "orderNumber": {
            "type": "integer",
            "description": "排序值",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "description": "书签名称"
          },
          "type": {
            "type": "string",
            "description": "书签类型",
            "enum": [
              "FOLDER",
              "ITEM"
            ]
          },
          "url": {
            "type": "string",
            "description": "书签地址"
          },
          "icon": {
            "type": "string",
            "description": "书签图标"
          },
          "color": {
            "type": "string",
            "description": "书签颜色"
          },
          "children": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BookmarkDomain"
            }
          }
        },
        "description": "书签领域对象"
      }
    }
  }
