  "paths": {
    "/login": {
      "post": {
        "tags": [
          "认证接口"
        ],
        "summary": "用户登录",
        "description": "Spring Security 默认登录接口",
        "requestBody": {
          "description": "登录参数",
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string",
                    "description": "用户名"
                  },
                  "password": {
                    "type": "string",
                    "description": "密码"
                  }
                }
              }
            },
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string",
                    "description": "用户名"
                  },
                  "password": {
                    "type": "string",
                    "description": "密码"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "登录成功，跳转首页"
          },
          "401": {
            "description": "用户名或密码错误"
          }
        }
      }
    },
    "/memo/update": {
      "post": {
        "tags": [
          "备忘管理"
        ],
        "summary": "更新备忘",
        "description": "根据id更新备忘",
        "operationId": "memoUpdate",
        "parameters": [
          {
            "name": "arg0",
            "in": "query",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/UpdateRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResult"
                }
              }
            }
          }
        }
      }
    },
    "/memo/delete/{id}": {
      "post": {
        "tags": [
          "备忘管理"
        ],
        "summary": "删除备忘",
        "description": "根据id删除备忘",
        "operationId": "memoDelete",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResult"
                }
              }
            }
          }
        }
      }
    },
