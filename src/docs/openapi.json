      "post": {
        "tags": [
          "认证接口"
        ],
        "summary": "用户登录",
        "description": "Spring Security 默认登录接口",
        "requestBody": {
          "description": "登录参数",
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "properties": {
                  "username": {
                    "type": "string",
                    "description": "用户名"
                  },
                  "password": {
                    "type": "string",
                    "description": "密码"
                  }
                }
              }
            },
            "application/json": {
              "schema": {
                "properties": {
                  "username": {
                    "type": "string",
                    "description": "用户名"
                  },
                  "password": {
                    "type": "string",
                    "description": "密码"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "登录成功，跳转首页"
          },
          "401": {
            "description": "用户名或密码错误"
          }
        }
      }
    },
