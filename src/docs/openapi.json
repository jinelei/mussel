      "post": {
        "tags": [
          "备忘管理"
        ],
        "summary": "新增备忘",
        "description": "新增备忘",
        "operationId": "memoCreate",
        "parameters": [
          {
            "name": "arg0",
            "in": "query",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/CreateRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResult"
                }
              }
            }
          }
        }
