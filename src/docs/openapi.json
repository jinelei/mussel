  "components": {
    "schemas": {
      "GenericResult": {
        "type": "object",
        "description": "通用响应对象",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32",
            "description": "错误代码"
          },
          "message": {
            "type": "string",
            "description": "错误信息"
          },
          "data": {
            "description": "响应数据"
          }
        }
      },
      "UpdateRequest": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "title": {
            "type": "string",
            "minLength": 1
          },
          "subTitle": {
            "type": "string",
            "minLength": 1
          },
          "content": {
            "type": "string",
            "minLength": 1
          },
          "orderNumber": {
            "type": "integer",
            "format": "int32"
          },
          "tagIds": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int64"
            }
          }
        },
        "required": [
          "content",
          "id",
          "orderNumber",
          "subTitle",
          "title"
        ]
      },
      "CreateRequest": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "minLength": 1
          },
          "subTitle": {
            "type": "string",
            "minLength": 1
          },
          "content": {
            "type": "string",
            "minLength": 1
          },
          "orderNumber": {
            "type": "integer",
            "format": "int32"
          },
          "tagIds": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int64"
            }
          }
        },
        "required": [
          "content",
          "subTitle",
          "title"
        ]
      },
      "BookmarkDomain": {
        "type": "object",
        "description": "书签领域对象",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64",
            "description": "主键ID"
          },
          "parentId": {
            "type": "integer",
            "format": "int64",
            "description": "父级ID"
          },
          "createTime": {
            "type": "string",
            "format": "date-time",
            "description": "创建时间"
          },
          "updateTime": {
            "type": "string",
            "format": "date-time",
            "description": "更新时间"
          },
          "orderNumber": {
            "type": "integer",
            "format": "int32",
            "description": "排序值"
          },
          "name": {
            "type": "string",
            "description": "书签名称"
          },
          "type": {
            "type": "string",
            "description": "书签类型",
            "enum": [
              "FOLDER",
              "ITEM"
            ]
          },
          "url": {
            "type": "string",
            "description": "书签地址"
          },
          "icon": {
            "type": "string",
            "description": "书签图标"
          },
          "color": {
            "type": "string",
            "description": "书签颜色"
          },
          "children": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BookmarkDomain"
            }
          }
        }
      },
      "ListRequest": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "description": "书签类型",
            "enum": [
              "FOLDER",
              "ITEM"
            ]
          },
          "url": {
            "type": "string"
          }
        }
      },
      "GetRequest": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          }
        },
        "required": [
          "id"
        ]
      },
      "DeleteRequest": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          }
        },
        "required": [
          "id"
        ]
      }
    }
